package br.com.banco;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import br.com.banco.converter.MyModelMapper;
import br.com.banco.data.model.Account;
import br.com.banco.data.vo.AccountVO;

@SpringBootApplication
public class ServicoBancoApplication {

	public static void main(String[] args) {
		SpringApplication.run(ServicoBancoApplication.class, args);
		
		AccountVO accountVO = new AccountVO();
		
		accountVO.setKey(123L);
		accountVO.setGender("Female");
		accountVO.setFullName("Carolina Alvino");
		accountVO.setEmailAdress("carolina.alvino02@gmail.com");
		accountVO.setAccountNumber(56785L);
		accountVO.setAccountBalance((long) 20.00);
		accountVO.setHomeAdress("Casa do caralho, ap 201");
		
		Account account = MyModelMapper.parseObject(accountVO, Account.class);
		
		System.out.print(account.getEmailAdress());
		System.out.print(account.getFirstName());
		System.out.print(account.getLastName());
		System.out.print(account.getAccountBalance());
		System.out.print(account.getAccountNumber());
		System.out.print(account.getId());
		
	}

}
